{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "jsx": "react",
    "target": "es2016", 
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  },
}